{
  "manifest_version": 3,
  "name": "Moodle Blind Review",
  "version": "1.1",
  "description": "Click the icon to toggle correct answers on/off.",
  "permissions": ["scripting", "tabs", "storage"],
  "host_permissions": [
  "<all_urls>"
],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Toggle Blind Review"
  }
}